MAS multiagent_jason {
	
    environment: c4jason.CartagoEnvironment

    agents:  
    	announcer	agentArchClass c4jason.CAgentArch;
    	initializer agentArchClass c4jason.CAgentArch;
   	 	car 		agentArchClass c4jason.CAgentArch #8;
   	 	drone 		agentArchClass c4jason.CAgentArch #4;
   	 	motorcycle 	agentArchClass c4jason.CAgentArch #8;
   	 	truck 		agentArchClass c4jason.CAgentArch #8;

	aslSourcePath: "src/asl/roles";
}
